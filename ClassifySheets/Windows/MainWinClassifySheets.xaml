					Style="{StaticResource Button.Base}"
					Click="BtnGo_OnClick"
					/>

			<Button x:Name="BtnExit"
					Content="Exit"
					Style="{StaticResource Button.Base}"
					Click="BtnExit_OnClick"
					/>
		</StackPanel>

		<Label Name="lbl1"
				Content="{Binding Path=Lbl1Content,
													FallbackValue='|'}"
				Grid.Column="0"
				Grid.Row="2"
				Margin="5,-2,0,0"
				Padding="0,0,0,2"
				HorizontalAlignment="Left"
				VerticalAlignment="Center"
				Foreground="White"
				/>

		<ProgressBar Name="Pb1"
				Grid.Column="0"
				Grid.Row="2"
				Margin="5,0,5,1"
				Height="2"
				VerticalAlignment="Bottom"
				HorizontalContentAlignment="Stretch"
				Value="{Binding Pb1Value}"
				Background="Transparent"
				BorderThickness="0"
				Maximum="{Binding Pb1MaximumValue}"
				ScrollViewer.VerticalScrollBarVisibility="Disabled"
				/>

		<Label Name="lbl2"
				Grid.Column="1"
				Grid.Row="2"
				Margin="5,-2,0,0"
				Padding="0,0,0,2"
				HorizontalAlignment="Left"
				VerticalAlignment="Center"
				Foreground="White"
				/>

		<ProgressBar Name="Pb2"
				Grid.Column="1"
				Grid.Row="2"
				Margin="5,0,5,0"
				Height="3"
				VerticalAlignment="Bottom"
				HorizontalContentAlignment="Stretch"
				Background="Transparent"
				BorderThickness="0"
				Maximum="{Binding Pb2MaximumValue}"
				ScrollViewer.VerticalScrollBarVisibility="Disabled"
				/>

		<TextBox Name="Tbx1Prog"
				Grid.Column="1"
				Grid.Row="1"
				Margin="5"
				FontFamily="Droid Sans Mono"
				Background="Transparent"
				Foreground="White"
				HorizontalScrollBarVisibility="Visible"
				Text="{Binding Path=Tbx1Progress,
											 Mode=OneWay}"
				VerticalScrollBarVisibility="Visible"
				/>

		<TextBox Name="Tbx1Msg"
				Grid.Column="0"
				Grid.Row="3"
				Grid.RowSpan="2"
				Margin="5,0,5,5"
				FontFamily="Droid Sans Mono"
				Background="Transparent"
				Foreground="White"
				HorizontalScrollBarVisibility="Visible"
				Text="{Binding Path=Tbx1Message,
											 Mode=OneWay}"
				VerticalScrollBarVisibility="Visible"
				/>

		<TextBox Name="Tbx2Msg"
				Grid.Column="1"
				Grid.Row="3"
				Grid.RowSpan="2"
				Margin="5,0,5,5"
				FontFamily="Droid Sans Mono"
				Background="Transparent"
				Foreground="White"
				HorizontalScrollBarVisibility="Visible"
				Text="{Binding Path=Tbx2Message,
											 Mode=OneWay}"
				VerticalScrollBarVisibility="Visible"
				/>

		<StackPanel x:Name="Sp_Header"
				Grid.Column="0"
				Grid.Row="0"
				Grid.ColumnSpan="2"
				Margin="5"
				Orientation="Vertical"
				Style="{StaticResource StackPanel.Base}"
				>

			<StackPanel Style="{StaticResource StackPanel.Horizontal}">

				<Label
						Content="ClassF Full Path"
						Width="{StaticResource Title.Width}"
						Style="{StaticResource Label.Base}"
						/>

				<Label
						Content="{Binding ElementName=MainWin,
															Mode=OneWay,
															FallbackValue='Path',
															Path=ClassificationFile.FullFilePath}"
						Style="{StaticResource Label.Base}"
						/>

			</StackPanel>

			<StackPanel Style="{StaticResource StackPanel.Horizontal}">

				<Label
						Content="Sample File Bldg"
						Width="{StaticResource Title.Width}"
						Style="{StaticResource Label.Base}"
						/>

				<Label
						Content="{Binding ElementName=MainWin,
															Mode=OneWay,
															FallbackValue='Bldg',
															Path=TestFileList.Building}"
						Style="{StaticResource Label.Base}"
						/>

			</StackPanel>

			<StackPanel Style="{StaticResource StackPanel.Horizontal}">

				<Label
						Content="Sample Full Path"
						Width="{StaticResource Title.Width}"
						Style="{StaticResource Label.Base}"
						/>

				<Label
						Content="{Binding ElementName=MainWin,
															Mode=OneWay,
															FallbackValue='Path',
															Path=ClassificationFile.SampleFilePath}"
						Style="{StaticResource Label.Base}"
						/>

			</StackPanel>
		</StackPanel>

		<StackPanel x:Name="Sp_data"
				Grid.Column="0"
				Grid.Row="1"
				Margin="5,0,0,0"
				Orientation="Vertical"
				Style="{StaticResource StackPanel.Base}"
				>

			<StackPanel
					Margin="5,0,0,0"
					Style="{StaticResource StackPanel.Horizontal}"
					>

				<Label
						Content="Base of Tree | Count"
						Width="{StaticResource Title.Width}"
						Style="{StaticResource Label.Base}"
						/>

				<Label
						Content="{Binding ElementName=MainWin,
															Mode=OneWay,
															FallbackValue='Count',
															Path=BaseOfTree.Children.Count}"
						Style="{StaticResource Label.Base}"
						/>

			</StackPanel>

			<StackPanel
					Margin="10,5,0,8"
					DataContext="{Binding ElementName=MainWin,
																Path=BaseOfTree.Children[1]}"
					Orientation="Vertical"
					Style="{StaticResource StackPanel.Base}"
					>

				<Label
						Content="Base of tree / node [1]"
						Margin="0,5,0,0"
						Style="{StaticResource Label.Base}"
						/>

				<StackPanel Style="{StaticResource StackPanel.Horizontal}">
					<Label
							Content="node[1] | Title"
							Width="{StaticResource Title.Width}"
							Style="{StaticResource Label.Base}"
							/>
					<Label
							Content="{Binding Path=Item.Title,
																Mode=OneWay,
																FallbackValue='Title'}"
							Style="{StaticResource Label.Base}"
							/>
				</StackPanel>

				<StackPanel Style="{StaticResource StackPanel.Horizontal}">
					<Label
							Content="node[1] | Desc."
							Width="{StaticResource Title.Width}"
							Style="{StaticResource Label.Base}"
							/>
					<Label
							Content="{Binding Path=Item.Description,
																Mode=OneWay,
																FallbackValue='Description'}"
							Style="{StaticResource Label.Base}"
							/>
				</StackPanel>
			</StackPanel>
		</StackPanel>

		<ProgressBar Name="PbProgress"
				Grid.Column="2"
				Grid.Row="2"
				Margin="5,0,14,0"
				Height="2"
				VerticalAlignment="Bottom"
				HorizontalContentAlignment="Stretch"
				Background="Transparent"
				BorderThickness="0"
				Maximum="{Binding Pb2MaximumValue}"
				ScrollViewer.VerticalScrollBarVisibility="Disabled"
				>

		</ProgressBar>

		<ProgressBar Name="PbTimer"
				Grid.Column="2"
				Grid.Row="2"
				Margin="5,0,14,0"
				Height="2"
				VerticalAlignment="Bottom"
				HorizontalContentAlignment="Stretch"
				Background="Transparent"
				BorderThickness="0"
				Maximum="{Binding Pb2MaximumValue}"
				ScrollViewer.VerticalScrollBarVisibility="Disabled"
				/>

		<TreeView x:Name="Tv1"
				Grid.Column="2"
				Grid.Row="3"
				Grid.RowSpan="2"
				Margin="5,0,5,5"
				HorizontalAlignment="Stretch"
				VerticalAlignment="Stretch"
				DataContext="{Binding Path=BaseOfTree}"
				d:DataContext="{x:Static sample:SampleData.TreeBase}"
				Background="Transparent"
				Foreground="White"
				ScrollViewer.HorizontalScrollBarVisibility="Visible"
				ScrollViewer.VerticalScrollBarVisibility="Visible"
				Visibility="Visible"
				ItemContainerStyle="{DynamicResource Tv1_ItemContainerStyle}"
				ItemTemplate="{DynamicResource Tv1_ItemTemplate}"
				ItemsSource="{Binding ChildrenView}"
				Style="{DynamicResource Tv1_TvStyle}"
				/>

	</Grid>

	<!--#endregion-->

</Window>
