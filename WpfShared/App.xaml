		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:local="clr-namespace:WpfShared"
		>
	<!--  StartupUri="Windows/MainWindow.xaml"  -->
	<Application.Resources>

		<ResourceDictionary>
