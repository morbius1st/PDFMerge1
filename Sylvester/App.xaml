<Application x:Name="app"
		x:Class="Sylvester.App"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:generic="clr-namespace:System.Collections;assembly=mscorlib"
		xmlns:local="clr-namespace:Sylvester"
		xmlns:specialized="clr-namespace:System.Collections.Specialized;assembly=System"
		xmlns:sys="clr-namespace:System;assembly=mscorlib"
		xmlns:user="clr-namespace:Sylvester.UserControls"
		StartupUri="MainWindow.xaml"
		>
	<Application.Resources>

		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Sylvester;component/Resources/XAML/MasterVisualResources.xaml" />
				<ResourceDictionary Source="/Sylvester;component/Resources/XAML/VisualStates.xaml" />
				<ResourceDictionary Source="/Sylvester;component/Resources/XAML/AppResources.xaml" />
				<ResourceDictionary Source="/Sylvester;component/Resources/XAML/SvgResources.xaml" />
				<ResourceDictionary Source="/Sylvester;component/Resources/XAML/NarrowScrollBar.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>

	</Application.Resources>

</Application>
